{
  "name": "UcuxMobileApp",
  "app_id": "8c4bc3d6",
  "proxies": [
    {
      "path": "/api/SecurityApi/",
      "proxyUrl": "http://ucux.co/api/SecurityApi/"
    }
  ],
  "watch": {
    "sass": [
      "scss/**/*.scss"
    ],
    "html": [
      "www/**/*.html"
    ],
    "livereload": [
      "www/**/*.html",
      "www/**/*.js",
      "www/**/*.css"
    ]
  },
  "gulpStartupTasks": [
    "sass",
    "watch"
  ],
  "watchPatterns": [
    "www/**/*",
    "!www/lib/**/*",
    "!www/**/*.map"
  ]
}
